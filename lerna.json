{
  "version": "0.21.4",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "packages/*"
  ],
  "command": {
    "version": {
      "allowBranch": [
        "main",
        "mui5"
      ],
      "message": "%s"
    }
  },
  "ignoreChanges": [
    "**/pkg/**",
    "**/__tests__/**"
  ]
}
