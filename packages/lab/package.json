{
  "name": "@superdispatch/ui-lab",
  "version": "0.16.0-alpha.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/superdispatch/ui.git"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "browser": "pkg/dist-web/index.js",
  "types": "src/index.ts",
  "scripts": {
    "version": "pika build"
  },
  "dependencies": {
    "@superdispatch/hooks": "^0.15.0",
    "@superdispatch/ui": "^0.16.0-alpha.1",
    "react-dropzone": "^11.3.4"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "react": "^17.0.2",
    "styled-components": "^5.2.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "pkg"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/*.spec.*",
            "**/*.stories.*",
            "**/*.playroom.*",
            "**/__tests__/**",
            "**/__testutils__/**"
          ]
        }
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-bundle-types"
      ]
    ]
  }
}
