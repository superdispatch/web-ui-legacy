{
  "name": "@superdispatch/hooks",
  "version": "0.10.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/superdispatch/ui.git"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "browser": "pkg/dist-web/index.js",
  "types": "src/index.ts",
  "scripts": {
